{
  "name": "dodoso",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "build": "npm run web:build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "DEBUG=dodoso:* node-dev index.js",
    "web:build": "browserify -t [ babelify --presets [ es2015 react ] ] -o public/js/bundle.js lib/bundle.js",
    "web:watch": "watchify -d -v -t [ babelify --presets [ es2015 react ] ] -o public/js/bundle.js lib/bundle.js",
    "db:latest": "knex migrate:latest",
    "db:rollback": "knex migrate:rollback",
    "db:reset": "npm run db:rollback && npm run db:latest"
  },
  "keywords": ["form", "questionnaire", "survey"],
  "author": "muddydixon<muddydixon@gmail.com>",
  "repository": "https://github.com/muddydixon/dodoso.git",
  "readmeFile": "README.md",
  "license": "Apache-2.0",
  "dependencies": {
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babelify": "^7.3.0",
    "body-parser": "^1.15.2",
    "bookshelf": "^0.10.0",
    "bootstrap": "^3.3.7",
    "browserify": "^13.1.0",
    "co": "^4.6.0",
    "config": "^1.21.0",
    "cookie-parser": "^1.4.3",
    "create-error": "^0.3.1",
    "debug": "^2.2.0",
    "express": "^4.14.0",
    "express-session": "^1.14.1",
    "flux": "^2.1.1",
    "font-awesome": "^4.6.3",
    "js-yaml": "^3.6.1",
    "knex": "^0.11.10",
    "mysql": "^2.11.1",
    "node-dev": "^3.1.3",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-router": "^2.7.0",
    "serve-static": "^1.11.1",
    "uuid": "^2.0.2",
    "watchify": "^3.7.0"
  }
}
